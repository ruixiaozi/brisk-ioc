{
  "name": "brisk-ioc",
  "version": "2.1.1",
  "description": "fast light brisk ioc/di container on nodejs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "node example/index.js",
    "compile": "tsc",
    "precompile": "rimraf ./lib && ts-extends-is --source=./src/interface --output=./src/interface.json",
    "postcompile": "copyfiles -u 1 \"./src/**/*\" -e \"./src/**/*.ts\" \"./lib\"",
    "upversion": "npm version patch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruixiaozi/brisk-ioc.git"
  },
  "keywords": [
    "fast",
    "light",
    "brisk",
    "ioc",
    "di",
    "node",
    "js",
    "spring"
  ],
  "author": "ruixiaozi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruixiaozi/brisk-ioc/issues"
  },
  "homepage": "https://github.com/ruixiaozi/brisk-ioc#readme",
  "dependencies": {
    "bluebird": "3.7.2",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13",
    "winston": "3.6.0",
    "winston-daily-rotate-file": "4.6.1"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "peerDependencies": {
    "@types/bluebird": ">=3.5.0",
    "@types/node": "*",
    "bluebird": ">=3.7.0",
    "ts-node": ">=10",
    "typescript": ">=4"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/bluebird": "3.5.36",
    "@types/lodash": "4.14.180",
    "@types/node": "16.11.10",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "brisk-ts-extends": "1.0.6",
    "copyfiles": "2.4.1",
    "dirlint": "1.0.1",
    "eslint": "8.10.0",
    "eslint-config-brisk": "1.0.2",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "rimraf": "3.0.2"
  }
}
